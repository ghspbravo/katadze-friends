#partners-header
    background-image url('../resourses/Partners/header-image.jpg')
    & h2
        color: white

.category-name
    text-align center
    margin-bottom: 100px
    & p  
        text-transform uppercase
    & :after
        content ' '

.category-delimiter hr
    border-color: secondaryDark
    
.tags 
    margin-top: 30px 

.project-pic
    width 100%

button.more-button
    OpenH4()
    border-bottom: 2px solid acsentAlt;
    padding: 10px 0;
    text-transform: uppercase;
    margin-top: 50px;

.list-card:nth-child(even) button.more-button
    border-bottom-color: acsent

